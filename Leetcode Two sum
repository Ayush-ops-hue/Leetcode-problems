#include <stdio.h>

int main() {
    
    int a[] = {2,7,8,3,6,1,4};
    int target=9;
    int n;
    n= sizeof(a);
     
    for(int i=0;i<=n;i++){
         for(int j=1;j<=n;j++){
             
             if(a[i]+a[j]==target){
            printf("Array index are %d and %d ",i,j);
            return 0;
         }
         }
    }
    
        printf("No indices");
        return 0;
    
    }
    
    
